    "graphql",
    "html",
    "javascript",
    "javascriptreact",
    "jest-snapshot",
    "json",
    "jsonc",
    "jsx",
    "jsx-attr",
    "jsx-tags",
    "markdown",
    "php",
    "plaintext",
    "scss",
    "styled-css",
    "text",
    "typescript",
    "typescriptreact",
    "xml",
    "yaml",
    "yml"
  ]
}
